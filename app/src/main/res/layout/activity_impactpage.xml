<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".impactpage">

    <EditText
        android:id="@+id/poundsoftrashoutoftheocean"
        android:layout_width="300dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Amount Of Trash Out Of The Ocean"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="45dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="How much marine wildlife have you saved:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="244dp" />

    <Button
        android:id="@+id/enter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/marinewildlifesavecount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="323dp"
        tools:layout_editor_absoluteY="244dp" />

    <TextView
        android:id="@+id/oceansavedsize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="323dp"
        tools:layout_editor_absoluteY="298dp" />

    <TextView
        android:id="@+id/humansavedcount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        tools:layout_editor_absoluteX="323dp"
        tools:layout_editor_absoluteY="347dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="How many humans have you saved: "
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="347dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="How much of the ocean have you saved:"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="298dp" />
</androidx.constraintlayout.widget.ConstraintLayout>